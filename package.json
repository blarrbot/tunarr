{
  "name": "dizquetv",
  "version": "1.0.0",
  "description": "Create LiveTV channels from your Plex media",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "build": "browserify ./web/app.js -p [ tsify ] -o ./web/public/bundle.js",
    "dev-client": "watchify ./web/app.js -o ./web/public/bundle.js",
    "dev-server": "nodemon index.js --ignore ./web/ --ignore ./db/ --ignore ./xmltv.xml",
    "compile": "babel index.js -d dist && babel src -d dist/src",
    "package": "sh ./make_dist.sh",
    "clean": "del-cli --force ./bin ./dist ./.dizquetv ./web/public/bundle.js",
    "dev": "nodemon -e ts --watch src --watch index.ts -e .json,.ts,.yml,.yaml,.js -x ts-node-esm -r tsconfig-paths/register --files --project tsconfig.json index.ts"
  },
  "type": "module",
  "main": "index.ts",
  "author": "vexorian",
  "license": "Zlib",
  "dependencies": {
    "angular": "^1.6.10",
    "angular-router-browserify": "0.0.2",
    "angular-vs-repeat": "2.0.13",
    "axios": "^1.6.0",
    "body-parser": "^1.19.0",
    "diskdb": "^0.1.17",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "fluent-ffmpeg": "^2.1.2",
    "JSONStream": "1.0.5",
    "lodash-es": "^4.17.21",
    "lowdb": "^6.1.1",
    "merge": "2.1.1",
    "node-graceful-shutdown": "1.1.0",
    "node-schedule": "^2.1.1",
    "node-ssdp": "^4.0.0",
    "random-js": "2.1.0",
    "request": "^2.88.2",
    "tslib": "^2.6.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xml-writer": "^1.7.0",
    "yargs": "^17.7.2"
  },
  "bin": "dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@types/express": "^4.17.20",
    "@types/express-fileupload": "^1.4.3",
    "@types/fluent-ffmpeg": "^2.1.23",
    "@types/lodash": "^4.14.200",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20.8.9",
    "@types/request": "^2.48.11",
    "@types/uuid": "^9.0.6",
    "@types/yargs": "^17.0.29",
    "browserify": "^16.5.1",
    "copyfiles": "^2.2.0",
    "del-cli": "^3.0.0",
    "nexe": "^4.0.0-rc.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsify": "^5.0.4",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "watchify": "^4.0.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
